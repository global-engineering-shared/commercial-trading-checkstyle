<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="MagicNumber" files=".*Test|.*TestBuilder\.java"/>
    <suppress checks="AbstractClassName" files=".*Test\.java"/>
    <suppress checks="MagicNumber" files=".*Benchmark\.java"/>
    <suppress checks="MagicNumber" files=".*InventoryMaintenanceRestApi\.java|.*InventoryRestApi\.java"/>
    <suppress checks="MagicNumber" files="sbe/.*\.java"/>
    <suppress checks="[a-zA-Z0-9]*" files="build/.*\.java"/>
    <suppress checks="LineLength" files=".*AzureTestConstants\.java"/>
</suppressions>
